<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".HomeSection.BasicDetails1Activity">

    <LinearLayout
        android:id="@+id/basic_details1"
        android:layout_width="0dp"
        android:layout_height="?actionBarSize"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/basic_detail1_img1"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:src="@drawable/ic_back_arrow" />

        <Button
            style="?android:attr/borderlessButtonStyle"
            android:id="@+id/edit1"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:layout_marginLeft="215dp"
            android:background="@drawable/forgot_pass_button_bg"
            android:focusableInTouchMode="true"
            android:gravity="center"
            android:paddingStart="0dp"
            android:text="Save"
            android:textColor="@color/white"
            android:textAllCaps="false" />


    </LinearLayout>

    <TextView
        android:id="@+id/bsaic_ddetail1_txt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans"
        android:text="Step 1 of 4"
        android:textAllCaps="true"
        android:textColor="#323232"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/basic_details1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/bsaic_ddetail1_txt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans_semibold"
        android:text="Basic Details"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bsaic_ddetail1_txt1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/bsaic_ddetail1_txt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans"
        android:text="Intent, Property Type &amp; Contact Details"
        android:textColor="#323232"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bsaic_ddetail1_txt2"
        app:layout_constraintVertical_bias="0.013" />

    <ImageView
        android:id="@+id/step1_logo"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:src="@drawable/step1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bsaic_ddetail1_txt3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/bsaic_ddetail1_txt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:fontFamily="@font/open_sans_semibold"
        android:text="What Kind Of Property?"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/step1_logo"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        style="?android:attr/borderlessButtonStyle"
        android:id="@+id/basic_detail1_txt5"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_change"
        android:text="Residental"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bsaic_ddetail1_txt4"
        app:layout_constraintVertical_bias="0.038" />


    <Button
        android:id="@+id/basic_detail1_txt6"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_change"
        android:gravity="center"
        android:text="Commercial"
        android:textColor="#323232"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/basic_detail1_txt5"
        app:layout_constraintTop_toBottomOf="@+id/bsaic_ddetail1_txt4"
        app:layout_constraintVertical_bias="0.038" />

    <TextView
        android:id="@+id/basic_detail1_txt7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans_semibold"
        android:text="Select Property Type"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/basic_detail1_txt5"
        app:layout_constraintVertical_bias="0.061" />


    <Button
        style="?android:attr/borderlessButtonStyle"
        android:id="@+id/basic_detail1_txt8"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_change"
        android:gravity="center"
        android:text="Apartment"
        android:textColor="#323232"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/basic_detail1_txt7"
        app:layout_constraintVertical_bias="0.041" />

    <Button
        style="?android:attr/borderlessButtonStyle"
        android:id="@+id/basic_detail1_txt9"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_change"
        android:gravity="center"
        android:text="Villa"
        android:textColor="#323232"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toEndOf="@+id/basic_detail1_txt8"
        app:layout_constraintTop_toBottomOf="@+id/basic_detail1_txt7"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        style="?android:attr/borderlessButtonStyle"
        android:id="@+id/basic_detail1_txt10"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_change"
        android:gravity="center"
        android:text="Builder Floor"
        android:textColor="#323232"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/basic_detail1_txt8"
        app:layout_constraintVertical_bias="0.057" />

    <Button
        style="?android:attr/borderlessButtonStyle"
        android:id="@+id/basic_detail1_txt11"
        android:layout_width="170dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_change"
        android:gravity="center"
        android:text="Service Apartment"
        android:layout_marginRight="10dp"
        android:textColor="#323232"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toEndOf="@+id/basic_detail1_txt10"
        app:layout_constraintTop_toBottomOf="@+id/basic_detail1_txt9"
        app:layout_constraintVertical_bias="0.054" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="280dp"
        android:scaleType="fitXY"
        android:src="@drawable/step_1_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/basic_detail1_txt11"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_save1"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:background="@drawable/forgot_pass_button_bg"
        android:drawableRight="@drawable/ic_arrow_forward"
        android:drawablePadding="20dp"
        android:text="Next"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.93" />


</androidx.constraintlayout.widget.ConstraintLayout>